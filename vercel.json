{
  "buildCommand": "cd apps/frontend && npm run build",
  "outputDirectory": "apps/frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "apps/backend/server.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/apps/backend/server.js"
    }
  ]
}
